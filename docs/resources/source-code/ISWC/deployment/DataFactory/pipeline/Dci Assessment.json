{
	"name": "Dci Assessment",
	"properties": {
		"activities": [
			{
				"name": "Copy Tsv",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[Azure].[DciAssessment]",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "SftpWriteSettings",
							"operationTimeout": "01:00:00",
							"useTempFileRename": true
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".tsv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "SourceDatabase",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 1
								}
							},
							{
								"source": {
									"name": "AgencyID",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 2
								}
							},
							{
								"source": {
									"name": "AgencyWorkCode",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 3
								}
							},
							{
								"source": {
									"name": "LinkedWorkIdentifier",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 4
								}
							},
							{
								"source": {
									"name": "Iswc",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 5
								}
							},
							{
								"source": {
									"name": "OriginalTitle",
									"type": "String"
								},
								"sink": {
									"type": "String",
									"ordinal": 6
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DciAssessmentSql",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DciAssessmentSftp",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "DCI"
		},
		"annotations": []
	}
}