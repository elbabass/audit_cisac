      "json": "openapi: 3.0.1\r\ninfo:\r\n  description: ISWC Database REST API - Label\r\n  version: \"1.0.0\"\r\n  title: ISWC Database REST API - Label\r\n  contact:\r\n    email: iswc.support@cisac.org\r\ntags:\r\n  - name: ISWC Label Submission\r\npaths:\r\n  '/submission/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Label Submission\r\n      summary: Array of ISWC submission objects that need to be added to the ISWC Database for a Label\r\n      operationId: addLabelSubmissionBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n              xml:\r\n                name: SubmissionBatch\r\n                wrapped: true\r\n        description: Array of ISWC submission objects that need to be added to the ISWC Database for a Label\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Submissions processed\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: Input malformed \r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\nexternalDocs:\r\n  description: Find out more about the ISWC Database\r\n  url: 'http://www.spanishpoint.ie'\r\ncomponents:\r\n  securitySchemes:\r\n    iswcdb_auth:\r\n      type: oauth2\r\n      flows:\r\n        implicit:\r\n          authorizationUrl: 'http://iswcapi.cisac.org/oauth/authorize'\r\n          scopes:\r\n            read_iswcs: read iswcs in your agency\r\n            write_iswcs: modify iswcs in your agency\r\n  schemas:\r\n    ISWCMetadata:\r\n      allOf:\r\n        - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n        - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n        - type: object\r\n          properties:\r\n            works:\r\n              type: array\r\n              items:\r\n                $ref: \"#/components/schemas/VerifiedSubmission\"\r\n            parentISWC:\r\n              description: \"A preferred ISWC of a parent of this ISWC. Presence of this field indicates that the ISWC merged to the parent.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n            linkedISWC:\r\n              description: \"All linked preferred ISWCs. Presence of this field indicates that those ISWCs merged to this ISWC.\"\r\n              type: array\r\n              items:\r\n                type: string\r\n                pattern: 'T[0-9]{10}'\r\n            overallParentISWC:\r\n              description: The preferred ISWC that is at the top of the chain of ISWCs.\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n          required:\r\n            - iswc\r\n            - agency\r\n          xml:\r\n            name: ISWCMetadata \r\n    Submission:\r\n      allOf:\r\n        - $ref: '#/components/schemas/CommonSubmissionAttributes'\r\n        - type: object\r\n          properties:\r\n            preferredIswc:\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n              description: \"The preferred ISWC\"\r\n            previewDisambiguation:\r\n              type: boolean\r\n              description: 'If this field is set to true, the submission will not be saved to the ISWC database if there are multiple matches. If this field is set to false or is not provided, the submission will be saved to the ISWC database.'\r\n            disableAddUpdateSwitching:\r\n              type: boolean\r\n              description: 'If this field is set to true, the submission will be rejected if a submission already exists for the provided Agency Work Code.'\r\n            allowProvidedIswc:\r\n              type: boolean\r\n              description: 'If this field is set to false, the submission will be rejected if the preferredIswc value cannot be found in the database. If this field is set to true and the preferredIswc value cannot be found in the database, a new ISWC record will be created with the value provided.'\r\n            detailLevel:\r\n              type: string\r\n              enum:\r\n                - full\r\n                - core\r\n              description: Allow the return of limited ISWC metadata\r\n    SubmissionResponse:\r\n      type: object\r\n      properties:\r\n        verifiedSubmission:\r\n          $ref: '#/components/schemas/VerifiedSubmission'\r\n        linkedIswcs:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              linkedReason:\r\n                type: integer\r\n                format: int32\r\n              iswcMetadata:\r\n                $ref: '#/components/schemas/ISWCMetadata'\r\n        potentialMatches:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/ISWCMetadata'\r\n        multipleAgencyWorkCodes:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              agency:\r\n                type: string\r\n                minLength: 3\r\n                maxLength: 3\r\n              workCode:\r\n                type: string\r\n                maxLength: 20\r\n              rejection:\r\n                $ref: '#/components/schemas/Rejection'\r\n    SubmissionBatch:\r\n      type: object\r\n      properties:\r\n        submissionId:\r\n          type: integer\r\n          format: int32\r\n          description: A unique submission identifier for this request. The same identifier will be populated for the submission in the response.\r\n        submission:\r\n          $ref: '#/components/schemas/Submission'\r\n        multipleAgencyWorkCodes:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              agency:\r\n                type: string\r\n                minLength: 3\r\n                maxLength: 3\r\n              workCode:\r\n                type: string\r\n                maxLength: 20\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n      xml:\r\n        name: Submission\r\n    VerifiedSubmission:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonSubmissionAttributes\"\r\n      - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n      - type: object\r\n      required:\r\n        - id\r\n        - iswc\r\n        - deleted\r\n      properties:\r\n        id:\r\n          description: \"An unique identifier assigned by the system\"\r\n          type: integer\r\n          format: int64\r\n        iswc:\r\n          description: \"A preferred ISWC assigned by the system\"\r\n        iswcEligible:\r\n          description: \"Indicator of a submitter being Iswc Eligible\"\r\n          type: boolean\r\n        iswcStatus:\r\n          description: \"Indicator of ISWC being preferred or provisional\"\r\n          type: string\r\n        deleted:\r\n          description: \"Indicator of a submission being logically deleted on CSINET\"\r\n          type: boolean\r\n        linkedFrom:\r\n          description: \"The ISWC record that this is linked from\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        linkedTo:\r\n          description: \"The ISWC record that this is linked to\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        rejection:\r\n          $ref: \"#/components/schemas/Rejection\"\r\n        archivedIswc:\r\n          description: \"The Archived ISWC\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        overallParentISWC:\r\n              description: \"The preferred ISWC that is at the top of the chain of ISWCs.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n    VerifiedSubmissionBatch:\r\n      type: object\r\n      properties:\r\n        submissionId:\r\n          type: integer\r\n          format: int32\r\n          description: The value of the submissionID from the request submission.\r\n        submission:\r\n          $ref: '#/components/schemas/SubmissionResponse'\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n        alternateIswcMatches:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/AlternateIswcMatch'\r\n      xml:\r\n        name: Submission\r\n    AlternateIswcMatch:\r\n      allOf:\r\n      - $ref: '#/components/schemas/CommonISWCMetadata'\r\n      - type: object  \r\n    CommonSubmissionAttributes:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n      - type: object\r\n      required:\r\n        - agency\r\n        - originalTitle\r\n        - interestedParties\r\n      properties:\r\n        agency:\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3\r\n        workcode:\r\n          type: string\r\n        disambiguation:\r\n          type: boolean\r\n        disambiguationReason:\r\n          $ref: '#/components/schemas/DisambiguationReason'\r\n        disambiguateFrom:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/DisambiguateFrom'\r\n        bvltr:\r\n          $ref: '#/components/schemas/BVLTR'\r\n        derivedWorkType