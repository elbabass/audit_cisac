      "json": "openapi: 3.0.1\r\ninfo:\r\n  description: ISWC Database REST API - Third Party\r\n  version: \"1.0.0\"\r\n  title: ISWC Database REST API - Third Party\r\n  contact:\r\n    email: iswc.support@cisac.org\r\ntags:\r\n  - name: ISWC Third Party Search\r\npaths:\r\n  '/searchByIswc/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Third Party Search\r\n      summary: Third Party - Find ISWC metadata for specified ISWCs\r\n      description: ''\r\n      operationId: thirdPartySearchByISWCBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/IswcSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/IswcSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of ISWC search objects\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/searchByAgencyWorkCode/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Third Party Search\r\n      summary: Third Party - Find ISWC metadata for specified batch of Agency and WorkCodes\r\n      description: ''\r\n      operationId: thirdPartySearchByAgencyWorkCodeBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/AgencyWorkCodeSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/AgencyWorkCodeSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of Agency Work Code search objects\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/searchByTitleAndContributor/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Third Party Search\r\n      summary: Third Party - Find ISWC metadata for specified Titles and Contributors\r\n      description: ''\r\n      operationId: thirdPartySearchByTitleAndContributorBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of ISWC search objects\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\nexternalDocs:\r\n  description: Find out more about the ISWC Database\r\n  url: 'http://www.spanishpoint.ie'\r\ncomponents:\r\n  securitySchemes:\r\n    iswcdb_auth:\r\n      type: oauth2\r\n      flows:\r\n        implicit:\r\n          authorizationUrl: 'http://iswcapi.cisac.org/oauth/authorize'\r\n          scopes:\r\n            read_iswcs: read iswcs in your agency\r\n            write_iswcs: modify iswcs in your agency\r\n  schemas:\r\n    ISWCMetadata:\r\n      allOf:\r\n        - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n        - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n        - type: object\r\n          properties:\r\n            works:\r\n              type: array\r\n              items:\r\n                $ref: \"#/components/schemas/VerifiedSubmission\"\r\n            parentISWC:\r\n              description: \"A preferred ISWC of a parent of this ISWC. Presence of this field indicates that the ISWC merged to the parent.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n            linkedISWC:\r\n              description: \"All linked preferred ISWCs. Presence of this field indicates that those ISWCs merged to this ISWC.\"\r\n              type: array\r\n              items:\r\n                type: string\r\n                pattern: 'T[0-9]{10}'\r\n            overallParentISWC:\r\n              description: The preferred ISWC that is at the top of the chain of ISWCs.\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n          required:\r\n            - iswc\r\n            - agency\r\n          xml:\r\n            name: ISWCMetadata\r\n    ISWCMetadataBatch:\r\n      type: object\r\n      properties:\r\n        searchId:\r\n          type: integer\r\n          format: int32\r\n          description: A unique identifier for this request. The same identifier will be populated for the ISWC Metadata request in the response.\r\n        searchResults:\r\n          type: array\r\n          items:\r\n            $ref: \"#/components/schemas/ISWCMetadata\"\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n      xml:\r\n        name: ISWCMetadata \r\n    VerifiedSubmission:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonSubmissionAttributes\"\r\n      - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n      - type: object\r\n      required:\r\n        - id\r\n        - iswc\r\n        - deleted\r\n      properties:\r\n        id:\r\n          description: \"An unique identifier assigned by the system\"\r\n          type: integer\r\n          format: int64\r\n        iswc:\r\n          description: \"A preferred ISWC assigned by the system\"\r\n        iswcEligible:\r\n          description: \"Indicator of a submitter being Iswc Eligible\"\r\n          type: boolean\r\n        deleted:\r\n          description: \"Indicator of a submission being logically deleted on CSINET\"\r\n          type: boolean\r\n        linkedFrom:\r\n          description: \"The ISWC record that this is linked from\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        linkedTo:\r\n          description: \"The ISWC record that this is linked to\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        rejection:\r\n          $ref: \"#/components/schemas/Rejection\"\r\n        archivedIswc:\r\n          description: \"The Archived ISWC\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        overallParentISWC:\r\n              description: \"The preferred ISWC that is at the top of the chain of ISWCs.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n    CommonSubmissionAttributes:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n      - type: object\r\n      required:\r\n        - agency\r\n        - sourcedb\r\n        - category\r\n        - originalTitle\r\n        - interestedParties\r\n      properties:\r\n        agency:\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3\r\n        sourcedb:\r\n          type: integer\r\n          format: int32\r\n        workcode:\r\n          type: string\r\n        category:\r\n          type: string\r\n          enum:\r\n          - DOM\r\n          - INT\r\n        disambiguation:\r\n          type: boolean\r\n        disambiguationReason:\r\n          $ref: '#/components/schemas/DisambiguationReason'\r\n        disambiguateFrom:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/DisambiguateFrom'\r\n        bvltr:\r\n          $ref: '#/components/schemas/BVLTR'\r\n        derivedWorkType:\r\n          $ref: '#/components/schemas/DerivedWorkType'\r\n        derivedFromIswcs:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/DerivedFrom'\r\n        performers:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/Performer'\r\n        instrumentation:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/Instrumentation'\r\n        cisnetCreatedDate:\r\n          description: \"Date and time when this metadata was created on CISNET\"\r\n          type: string\r\n          format: date-time\r\n        cisnetLastModifiedDate:\r\n          description: \"Date and time when this metadata last modified on CISNET\"\r\n          type: string\r\n          format: date-time\r\n        additionalIdentifiers:\r\n          description: \"Optional list of additional identifiers that can be supplied with the submission.\"\r\n          type: object\r\n          properties:\r\n            isrcs:\r\n              type: array\r\n              items:\r\n                type: string\r\n            publisherIdentifiers:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  nameNumber:\r\n                    type: integer\r\n                    format: int64\r\n                  submitterCode:\r\n                    type: string\r\n                  workCode:\r\n                    type: array\r\n                    items:\r\n                      type: string\r\n            agencyWorkCodes:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  agency:\r\n                    type: string\r\n                    minLength: 3\r\n                    maxLength: 3\r\n                  workCode:\r\n                    type: string\r\n                    maxLength: 20\r\n            labelIdentifiers:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  submitterDPID:\r\n                    type: string\r\n                  workCode:\r\n                    type: array\r\n                    items:\r\n                      type: string     \r\n            recordings:\r\n              type: array\r\n              items:\r\n                type: object\r\n                required: \r\n                  - isrc\r\n                  - recordingTitle\r\n                properties:\r\n                  isrc:\r\n                    type: string\r\n                  recordingTitle:\r\n                    type: string\r\n                  subTitle:\r\n                    type: string\r\n                  labelName:\r\n                    type: string\r\n                  releaseEmbargoDate:\r\n                    type: string\r\n                    format: date-time\r\n                  performers:\r\n                    type: array\r\n                    items:\r\n                      $ref: '#/components/schemas/Performer'\r\n                  \r\n    CommonISWCMetadata:\r\n      required:\r\n        - originalTitle\r\n        - interestedParties\r\n      properties:\r\n        iswc:\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        iswcStatus:\r\n          description: \"Indicator of ISWC being preferred, provisional or archived\"\r\n          type: string\r\n        agency:\r\n          description: A CISAC code of a submitting agency that allocated this ISWC\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3      \r\n        originalTitle:\r\n          description: \"Original title of a musical composition\"\r\n          type: string\r\n        otherTitles:\r\n          description: \"Other titles associated with musical composition (aka alternative titles)\"\r\n          type: array\r\n          items:\r\n            $ref: \"#/components/schemas/Title\"\r\n        interestedParties:\r\n          description: \"Interested party associated with musical composition, such as :composers, lyricist, translator, original and sub publishers\"\r\n          type: array\r\n          items:\r\n            $ref: \"#/components/schemas/InterestedParty\"\r\n        recordings:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              isrc:\r\n                type: string\r\n              recordingTitle:\r\n                type: string\r\n              subTitle:\r\n                type: string\r\n              labelName:\r\n                type: string\r\n              releaseEmbargoDate:\r\n                type: string\r\n                format: date-time\r\n              performers:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/Performer'\r\n    CommonSystemAttributes:\r\n      properties:\r\n        createdDate:\r\n          description: \"Data and time of creation in the system\"\r\n          type: string\r\n          format: date-time\r\n        lastModifiedDate:\r\n          description: \"Data and time of the latest update in the system\"\r\n          type: string\r\n          format: date-time\r\n        lastModifiedBy:\r\n          description: \"Identifier of the last update in the system\"\r\n          type: string\r\n    Rejection:\r\n      required:\r\n        - code\r\n        - message\r\n      properties:\r\n        code:\r\n          type: string\r\n        message:\r\n          type: string\r\n    Title:\r\n      type: object\r\n      required:\r\n        - title\r\n        - type\r\n      properties:\r\n        title:\r\n          description: Musical work title\r\n          type: string\r\n        type:\r\n          description: Musical work title type\r\n          type: string\r\n          enum:\r\n            - CT\r\n            - OT\r\n            - RT\r\n            - AT\r\n            - ET\r\n            - ST\r\n            - TO\r\n            - OA\r\n            - TE\r\n            - FT\r\n            - IT\r\n            - TT\r\n            - PT\r\n            - OL\r\n            - AL\r\n    InterestedParty:\r\n      type: object\r\n      properties:\r\n        name:\r\n          type: string\r\n        lastName:\r\n          type: string\r\n        nameNumber:\r\n          type: integer\r\n          format: int64\r\n        affiliation:\r\n          type: string\r\n        role:\r\n          type: string\r\n          description: 'The role will be rolled up to the corresponding CISAC Role as per the following mapping: C(C,A,CA), MA(AR,SR), TA(AD,SA,TR) E(E,AM)'\r\n          enum:\r\n            - CA\r\n            - AR\r\n            - SE\r\n            - PA\r\n            - ES\r\n            - AM\r\n            - SA\r\n            - C\r\n            - AD\r\n            - A\r\n            - E\r\n            - AQ\r\n            - SR\r\n            - TR\r\n            - MA\r\n            - TA\r\n        legalEntityType:\r\n          type: string\r\n          enum:\r\n          - N\r\n          - L\r\n    Performer:\r\n      type: object\r\n      required:\r\n        - lastName\r\n      properties:\r\n        isni:\r\n          type: string\r\n          minLength: 1\r\n          maxLength: 20\r\n        ipn:\r\n          type: integer\r\n          format: int64\r\n        firstName:\r\n          type: string\r\n          minLength: 1\r\n          maxLength: 50\r\n        lastName:\r\n          type: string\r\n          minLength: 1\r\n          maxLength: 50\r\n        designation:\r\n          type: string\r\n          enum:\r\n          - Main Artist\r\n          - Other\r\n    Instrumentation:\r\n      type: object\r\n      required:\r\n        - code\r\n      properties:\r\n        code:\r\n          type: string\r\n          minLength: 1\r\n          maxLength: 3\r\n    DerivedFrom:\r\n      type: object\r\n      properties:\r\n        iswc:\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        title:\r\n          type: string\r\n    DisambiguateFrom:\r\n      type: object\r\n      properties:\r\n        iswc:\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n    DisambiguationReason:\r\n      type: string\r\n      description: Disambiguation Reason Code\r\n      enum:\r\n        - DIT\r\n        - DIA\r\n        - DIE\r\n        - DIC\r\n        - DIP\r\n        - DIV\r\n    BVLTR:\r\n      type: string\r\n      description: \"Background, Logo, Theme, Visual or Rolled Up Cue\"\r\n      enum:\r\n        - Background\r\n        - Logo\r\n        - Theme\r\n        - Visual\r\n        - RolledUpCue\r\n    DerivedWorkType:\r\n      type: string\r\n      description: \"Derived Work Type- if not provided then this isn't a derived work\"\r\n      enum:\r\n        - ModifiedVersion\r\n        - Excerpt\r\n        - Composite\r\n    IswcSearchModel:\r\n      type: object\r\n      properties:\r\n        iswc:\r\n          description: The preferred ISWC\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n    AgencyWorkCodeSearchModel:\r\n      type: object\r\n      properties:\r\n        agency:\r\n          description: The Agency of the ISWC submission\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3\r\n        workCode:\r\n          description: The Agency WorkCode of the ISWC submission\r\n          type: string\r\n          maxLength: 20\r\n    TitleAndContributorSearchModel:\r\n      type: object\r\n      properties:\r\n        titles:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/Title'\r\n        interestedParties:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/InterestedParty'",
