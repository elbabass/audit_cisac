    <EmbeddedResource Remove="Services\**" />
    <None Remove="Services\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Configuration\Swagger\SpanishPoint.Azure.Iswc.Api.Agency.xml" />
