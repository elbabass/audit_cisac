<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <RootNamespace>SpanishPoint.Azure.Iswc.Api.Agency</RootNamespace>
    <AssemblyName>SpanishPoint.Azure.Iswc.Api.Agency</AssemblyName>
    <UserSecretsId>dc51cb57-6fe1-4807-99eb-c40300421987</UserSecretsId>
