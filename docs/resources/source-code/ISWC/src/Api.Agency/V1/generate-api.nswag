  "documentGenerator": {
    "fromDocument": {
      "json": "openapi: 3.0.1\r\ninfo:\r\n  description: ISWC Database REST API - Agency\r\n  version: \"1.0.0\"\r\n  title: ISWC Database REST API - Agency\r\n  contact:\r\n    email: iswc.support@cisac.org\r\ntags:\r\n  - name: ISWC Submission\r\n  - name: ISWC Search\r\n  - name: ISWC Merge\r\npaths:\r\n  /submission:\r\n    post:\r\n      tags:\r\n        - ISWC Submission\r\n      summary: Add a new ISWC submission to the database\r\n      operationId: addSubmission\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              $ref: '#/components/schemas/Submission'\r\n          application/xml:\r\n            schema:\r\n              $ref: '#/components/schemas/Submission'\r\n        description: ISWC submission object that needs to be added to the ISWC Database\r\n        required: true\r\n      responses:\r\n        '201':\r\n          description: \"Submission added\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                $ref: '#/components/schemas/SubmissionResponse'\r\n            application/xml:\r\n              schema:\r\n                $ref: '#/components/schemas/SubmissionResponse'\r\n        '400':\r\n          description: Input malformed \r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n    put:\r\n      tags:\r\n        - ISWC Submission\r\n      summary: Update an existing ISWC submission\r\n      description: ''\r\n      operationId: updateSubmission\r\n      parameters:\r\n        - name: preferredIswc\r\n          in: query\r\n          description: The preferred ISWC\r\n          required: false\r\n          schema:\r\n            type: string\r\n            pattern: 'T[0-9]{10}'\r\n      requestBody:\r\n        description: ISWC submission object that needs to be updated in the ISWC Database\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              $ref: '#/components/schemas/Submission'\r\n          application/xml:\r\n            schema:\r\n              $ref: '#/components/schemas/Submission'\r\n      responses:\r\n        '200':\r\n          description: \"Submission updated\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                $ref: '#/components/schemas/SubmissionResponse'\r\n            application/xml:\r\n              schema:\r\n                $ref: '#/components/schemas/SubmissionResponse'\r\n        '400':\r\n          description: Input malformed\r\n        '404':\r\n          description: Submission not found\r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n    delete:\r\n      tags:\r\n        - ISWC Submission\r\n      summary: Deletes an ISWC submission\r\n      operationId: deleteSubmission\r\n      parameters:\r\n        - name: preferredIswc\r\n          in: query\r\n          description: The preferred ISWC\r\n          required: true\r\n          schema:\r\n            type: string\r\n            pattern: 'T[0-9]{10}'\r\n        - name: agency\r\n          in: query\r\n          description: Agency of ISWC submission to delete\r\n          required: true\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n        - name: workcode\r\n          in: query\r\n          description: Agency internal workcode for work as previously submitted\r\n          required: true\r\n          schema:\r\n            type: string\r\n        - name: sourceDb\r\n          in: query\r\n          description: SourceDb of ISWC submission to delete\r\n          required: true\r\n          schema:\r\n            type: integer\r\n            format: int32\r\n        - name: reasonCode\r\n          in: query\r\n          description: Reason Code for the deletion\r\n          required: true\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n      responses:\r\n        '204':\r\n          description: Iswc submission successfully deleted\r\n        '400':\r\n          description: Invalid agency and work code provided\r\n        '404':\r\n          description: ISWC submission not found\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n  '/submission/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Submission\r\n      summary: Add a batch of new ISWC submissions to the database\r\n      operationId: addSubmissionBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n              xml:\r\n                name: SubmissionBatch\r\n                wrapped: true\r\n        description: Array of ISWC submission objects that need to be added to the ISWC Database\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Submissions processed\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: Input malformed \r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n    put:\r\n      tags:\r\n        - ISWC Submission\r\n      summary: Update a batch of existing ISWC submissions\r\n      description: ''\r\n      operationId: updateSubmissionBatch\r\n      requestBody:\r\n        description: ISWC submission objects that need to be updated in the ISWC Database\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n              xml:\r\n                name: SubmissionBatch\r\n                wrapped: true\r\n      responses:\r\n        '207':\r\n          description: \"Submissions processed\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: Input malformed\r\n        '404':\r\n          description: Submission not found\r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n  '/allocation/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Allocation and Resolution\r\n      summary: Allocate a batch of new ISWC submissions\r\n      operationId: addAllocationBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n              xml:\r\n                name: SubmissionBatch\r\n                wrapped: true\r\n        description: Array of ISWC allocation objects that need to be added to the ISWC Database\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Submissions processed\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: Input malformed \r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n  '/resolution/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Allocation and Resolution\r\n      summary: Resolve a batch of ISWC submissions\r\n      operationId: addResolutionBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/SubmissionBatch'\r\n              xml:\r\n                name: SubmissionBatch\r\n                wrapped: true\r\n        description: Array of ISWC submission objects that need to be resolved in the ISWC Database\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Submissions processed\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/VerifiedSubmissionBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: Input malformed \r\n        '422':\r\n          description: Input validation error\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs          \r\n  '/iswc/searchByIswc':\r\n    get:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified ISWC\r\n      description: ''\r\n      operationId: searchByISWC\r\n      parameters:\r\n        - name: iswc\r\n          in: query\r\n          description: The preferred ISWC\r\n          required: true\r\n          schema:\r\n            type: string\r\n            pattern: 'T[0-9]{10}'\r\n      responses:\r\n        '200':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                $ref: \"#/components/schemas/ISWCMetadata\"\r\n            application/xml:\r\n              schema:\r\n                $ref: \"#/components/schemas/ISWCMetadata\"\r\n        '400':\r\n          description: \"Invalid ISWC supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/iswc/searchByIswc/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified ISWCs\r\n      description: ''\r\n      operationId: searchByISWCBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/IswcSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/IswcSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of ISWC search objects\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid ISWC supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/iswc/searchByAgencyWorkCode':\r\n    get:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified Agency and WorkCode\r\n      description: ''\r\n      operationId: searchByAgencyWorkCode\r\n      parameters:\r\n        - name: agency\r\n          in: query\r\n          description: The Agency of the ISWC submission\r\n          required: true\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n        - name: workCode\r\n          in: query\r\n          description: The Agency WorkCode of the ISWC submission\r\n          required: true\r\n          schema:\r\n            type: string\r\n            maxLength: 20\r\n        - name: detailLevel\r\n          in: query\r\n          description: Allow the return of limited ISWC metadata\r\n          required: false\r\n          schema:\r\n            type: string\r\n            enum:\r\n            - full\r\n            - minimal\r\n            - coreAndLinks\r\n            - core\r\n      responses:\r\n        '200':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/ISWCMetadata\"\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/ISWCMetadata\"\r\n                xml:\r\n                  name: Result\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid Agency code supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/iswc/searchByAgencyWorkCode/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified batch of Agency and WorkCodes\r\n      description: ''\r\n      operationId: searchByAgencyWorkCodeBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/AgencyWorkCodeSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/AgencyWorkCodeSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of Agency Work Code search objects\r\n        required: true\r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid Agency code supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/iswc/searchByTitleAndContributor':\r\n    post:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified Titles and Contributors\r\n      description: ''\r\n      operationId: searchByTitleAndContributor\r\n      requestBody:\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n          application/xml:\r\n            schema:\r\n              $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n      responses:\r\n        '200':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/ISWCMetadata\"\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/ISWCMetadata\"\r\n                xml:\r\n                  name: Result\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid ISWC Search Model supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/iswc/searchByTitleAndContributor/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Search\r\n      summary: Find ISWC metadata for specified batch of Titles and Contributors\r\n      description: ''\r\n      operationId: searchByTitleAndContributorBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/TitleAndContributorSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of ISWC search objects\r\n        required: true        \r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid ISWC Search Model supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n  '/usage/search/batch':\r\n    post:\r\n      tags:\r\n        - ISWC Usage Search\r\n      summary: Usage Search - Find ISWC metadata for specified batch of Titles and Contributors\r\n      description: ''\r\n      operationId: usageSearchBatch\r\n      requestBody:\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/UPSearchModel'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items:\r\n                $ref: '#/components/schemas/UPSearchModel'\r\n              xml:\r\n                name: SearchBatch\r\n                wrapped: true\r\n        description: Array of ISWC search objects\r\n        required: true        \r\n      responses:\r\n        '207':\r\n          description: \"Iswc metadata found\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/ISWCMetadataBatch'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: \"Invalid ISWC Search Model supplied\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs \r\n  '/iswc/merge':\r\n    post:\r\n      tags:\r\n        - ISWC Merge\r\n      summary: Merge ISWC meta-data associated with a provided list of ISWCs or Society Work Codes and as part of the merge have an ISWC designated as preferred.\r\n      description: ''\r\n      operationId: mergeISWCMetadata\r\n      parameters: \r\n        - name: preferredIswc\r\n          in: query\r\n          description: The preferred ISWC\r\n          required: true\r\n          schema:\r\n            type: string\r\n            pattern: 'T[0-9]{10}'\r\n        - name: agency\r\n          in: query\r\n          description: The agency submitting the merge request\r\n          required: true\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n      requestBody:\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: object\r\n              properties:\r\n                iswcs:\r\n                  type: array\r\n                  items:\r\n                    type: string\r\n                    pattern: 'T[0-9]{10}'\r\n                agencyWorks:\r\n                  type: array\r\n                  items:\r\n                    $ref: '#/components/schemas/WorkNumber'\r\n          application/xml:\r\n            schema:\r\n              type: object\r\n              properties:\r\n                iswcs:\r\n                  type: array\r\n                  items:\r\n                    type: string\r\n                    pattern: 'T[0-9]{10}'\r\n                agencyWorks:\r\n                  type: array\r\n                  items:\r\n                    $ref: '#/components/schemas/WorkNumber'\r\n              xml:\r\n                name: MergeRequest\r\n      responses:\r\n        '204':\r\n          description: \"ISWC Metadata merged and ISWC designated as preferred\"\r\n        '400':\r\n          description: \"Bad request\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n    delete:\r\n      tags:\r\n        - ISWC Merge\r\n      summary: Delete a set of incorrect metadata from a single Preferred ISWC that was incorrectly merged / matched previously. \r\n      operationId: demergeISWCMetadata\r\n      parameters: \r\n        - name: preferredIswc\r\n          in: query\r\n          description: The preferred ISWC\r\n          required: true\r\n          schema:\r\n            type: string\r\n            pattern: 'T[0-9]{10}'\r\n        - name: agency\r\n          in: query\r\n          description: Agency of iswc submission to delete\r\n          required: true\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n        - name: workcode\r\n          in: query\r\n          description: Agency internal workcode for work as previously submitted\r\n          required: true\r\n          schema:\r\n            type: string\r\n      responses:\r\n        '204':\r\n          description: \"ISWC Metadata demerged\"\r\n        '400':\r\n          description: \"Bad request\"\r\n        '404':\r\n          description: \"ISWC not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs            \r\n  '/iswc/workflowTasks':\r\n    get:\r\n      tags:\r\n        - ISWC Workflow Tasks\r\n      summary: Retrieve a list of outstanding Workflow Tasks associated with an Agency\r\n      operationId: findWorkflowTasks\r\n      parameters:\r\n        - name: agency\r\n          in: query\r\n          required: true\r\n          description: CISAC Agency identifier\r\n          schema:\r\n            type : string\r\n            minLength: 3\r\n            maxLength: 3\r\n        - name: showWorkflows\r\n          in: query\r\n          required: true\r\n          description: Workflows assigned to or created by my Agency\r\n          schema:\r\n            type: string\r\n            enum:\r\n            - AssignedToMe\r\n            - CreatedByMe\r\n        - name: workflowType\r\n          in: query\r\n          required: false\r\n          description: Type of Workflow\r\n          schema:\r\n            type: string\r\n            enum:\r\n            - UpdateApproval\r\n            - MergeApproval\r\n            - DemergeApproval\r\n        - name: status\r\n          in: query\r\n          required: false\r\n          description: Workflow Status\r\n          schema:\r\n            type: array\r\n            items:\r\n              type: string\r\n              title: WorkflowStatus\r\n              enum:\r\n                - Outstanding\r\n                - Approved\r\n                - Rejected\r\n                - Cancelled\r\n        - name: startIndex\r\n          in: query\r\n          description: Start index for paging\r\n          schema:\r\n            type: integer\r\n            format: int32\r\n            minimum: 0\r\n        - name: pageLength\r\n          in: query\r\n          description: Number of records to return\r\n          schema:\r\n            type: integer\r\n            format: int32\r\n            minimum: 1\r\n            maximum: 1000\r\n        - name: fromDate\r\n          in: query\r\n          description: Left bound on date range\r\n          schema:\r\n            type: string\r\n            format: YYYY-MM-DD\r\n        - name: toDate\r\n          in: query\r\n          description: Right bound on date range\r\n          schema:\r\n            type: string\r\n            format: YYYY-MM-DD\r\n        - name: iswc\r\n          in: query\r\n          description: Filter by ISWC\r\n          schema:\r\n            type: string\r\n        - name: agencyWorkCodes\r\n          in: query\r\n          description: Filter by Agency Work Codes - seperated by \\';\\'\r\n          schema:\r\n            type: string\r\n        - name: originatingAgency\r\n          in: query\r\n          description: Filter by Originating Agency\r\n          schema:\r\n            type: string\r\n            minLength: 3\r\n            maxLength: 3\r\n      responses:\r\n        '200':\r\n          description: \"List of outstanding Workflow Tasks\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/WorkflowTask\"\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: \"#/components/schemas/WorkflowTask\"\r\n                xml:\r\n                  name:\r\n                    CorrectionTask\r\n        '401':\r\n          description: \"Not Authorized\"\r\n        '404':\r\n          description: \"Agency not found\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n    patch:\r\n      tags:\r\n        - ISWC Workflow Tasks\r\n      summary: Update the status of an outstanding Workflow Tasks associated with an Agency\r\n      operationId: updateWorkflowTask\r\n      parameters:\r\n        - name: agency\r\n          in: query\r\n          required: true\r\n          description: CISAC Agency identifier\r\n          schema:\r\n            type : string\r\n            minLength: 3\r\n            maxLength: 3\r\n      requestBody:\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              type: array\r\n              items: \r\n                $ref: '#/components/schemas/WorkflowTaskUpdate'\r\n          application/xml:\r\n            schema:\r\n              type: array\r\n              items: \r\n                $ref: '#/components/schemas/WorkflowTaskUpdate'\r\n      responses:\r\n        '207':\r\n          description: 'Workflow Task updated'\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/WorkflowTask'\r\n            application/xml:\r\n              schema:\r\n                type: array\r\n                items:\r\n                  $ref: '#/components/schemas/WorkflowTask'\r\n                xml:\r\n                  name: Batch\r\n                  wrapped: true\r\n        '400':\r\n          description: 'Bad Request'\r\n        '401':\r\n          description: \"Not Authorized\"\r\n        '404':\r\n          description: \"Workflow Task not found\"\r\n      security:\r\n        - iswcdb_auth: \r\n            - read_iswcs\r\n            - write_iswcs\r\n  '/agent/notification':\r\n    get:\r\n      tags:\r\n        - ISWC Agent\r\n      summary: Retrieve a list of CSN notifications associated with an Agency\r\n      operationId: getNotifications\r\n      parameters:\r\n        - name: agency\r\n          in: query\r\n          required: true\r\n          description: CISAC Agency identifier\r\n          schema:\r\n            type : string\r\n            minLength: 3\r\n            maxLength: 3\r\n        - name: csnTransactionType\r\n          in: query\r\n          required: true\r\n          description: Type of transaction requested\r\n          schema:\r\n            type: string\r\n            enum:\r\n            - MER\r\n            - DMR\r\n        - name: fromDate\r\n          in: query\r\n          required: true\r\n          description: The date from which to retrieve notifications\r\n          schema:\r\n            type: string\r\n            format: YYYY-MM-DD\r\n        - name: pageLength\r\n          in: query\r\n          required: true\r\n          description: Number of records to return\r\n          schema:\r\n            type: integer\r\n            format: int32\r\n            minimum: 1\r\n            maximum: 100\r\n        - name: continuationToken\r\n          in: query\r\n          description: Used to page through results\r\n          schema:\r\n            type: string\r\n      responses:\r\n        '200':\r\n          description: \"List of notifications\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                $ref: \"#/components/schemas/AgentNotificationResponse\"\r\n            application/xml:\r\n              schema:\r\n                $ref: \"#/components/schemas/AgentNotificationResponse\"\r\n        '401':\r\n          description: \"Not Authorized\"\r\n        '400':\r\n          description: \"Bad Request\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\n  '/agent/run':\r\n    post:\r\n      tags:\r\n        - ISWC Agent\r\n      summary: Notify ISWC when an agent run starts and ends\r\n      operationId: updateAgentRun\r\n      requestBody:\r\n        required: true\r\n        content:\r\n          application/json:\r\n            schema:\r\n              $ref: '#/components/schemas/AgentRun'\r\n          application/xml:\r\n            schema:\r\n              $ref: '#/components/schemas/AgentRun'\r\n      responses:\r\n        '200':\r\n          description: \"Agent run Id\"\r\n          content:\r\n            application/json:\r\n              schema:\r\n                type: object\r\n                properties:\r\n                  runId:\r\n                    type: string\r\n            application/xml:\r\n              schema:\r\n                type: object\r\n                properties:\r\n                  runId:\r\n                    type: string\r\n                xml:\r\n                  name:\r\n                    CsnNotification\r\n        '401':\r\n          description: \"Not Authorized\"\r\n        '400':\r\n          description: \"Bad Request\"\r\n      security:\r\n        - iswcdb_auth:\r\n            - read_iswcs\r\n            - write_iswcs\r\nexternalDocs:\r\n  description: Find out more about the ISWC Database\r\n  url: 'http://www.spanishpoint.ie'\r\ncomponents:\r\n  securitySchemes:\r\n    iswcdb_auth:\r\n      type: oauth2\r\n      flows:\r\n        implicit:\r\n          authorizationUrl: 'http://iswcapi.cisac.org/oauth/authorize'\r\n          scopes:\r\n            read_iswcs: read iswcs in your agency\r\n            write_iswcs: modify iswcs in your agency\r\n  schemas:\r\n    ISWCMetadata:\r\n      allOf:\r\n        - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n        - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n        - type: object\r\n          properties:\r\n            works:\r\n              type: array\r\n              items:\r\n                $ref: \"#/components/schemas/VerifiedSubmission\"\r\n            parentISWC:\r\n              description: \"A preferred ISWC of a parent of this ISWC. Presence of this field indicates that the ISWC merged to the parent.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n            linkedISWC:\r\n              description: \"All linked preferred ISWCs. Presence of this field indicates that those ISWCs merged to this ISWC.\"\r\n              type: array\r\n              items:\r\n                type: string\r\n                pattern: 'T[0-9]{10}'\r\n            overallParentISWC:\r\n              description: The preferred ISWC that is at the top of the chain of ISWCs.\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n          required:\r\n            - iswc\r\n            - agency\r\n          xml:\r\n            name: ISWCMetadata\r\n    ISWCMetadataBatch:\r\n      type: object\r\n      properties:\r\n        searchId:\r\n          type: integer\r\n          format: int32\r\n          description: A unique identifier for this request. The same identifier will be populated for the ISWC Metadata request in the response.\r\n        searchResults:\r\n          type: array\r\n          items:\r\n            $ref: \"#/components/schemas/ISWCMetadata\"\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n      xml:\r\n        name: ISWCMetadata \r\n    Submission:\r\n      allOf:\r\n        - $ref: '#/components/schemas/CommonSubmissionAttributes'\r\n        - type: object\r\n          properties:\r\n            preferredIswc:\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n              description: \"The preferred ISWC\"\r\n            previewDisambiguation:\r\n              type: boolean\r\n              description: 'If this field is set to true, the submission will not be saved to the ISWC database if there are multiple matches. If this field is set to false or is not provided, the submission will be saved to the ISWC database.'\r\n            disableAddUpdateSwitching:\r\n              type: boolean\r\n              description: 'If this field is set to true, the submission will be rejected if a submission already exists for the provided Agency Work Code.'\r\n            allowProvidedIswc:\r\n              type: boolean\r\n              description: 'If this field is set to false, the submission will be rejected if the preferredIswc value cannot be found in the database. If this field is set to true and the preferredIswc value cannot be found in the database, a new ISWC record will be created with the value provided.'\r\n            detailLevel:\r\n              type: string\r\n              enum:\r\n                - full\r\n                - core\r\n              description: Allow the return of limited ISWC metadata\r\n    SubmissionResponse:\r\n      type: object\r\n      properties:\r\n        verifiedSubmission:\r\n          $ref: '#/components/schemas/VerifiedSubmission'\r\n        linkedIswcs:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              linkedReason:\r\n                type: integer\r\n                format: int32\r\n              iswcMetadata:\r\n                $ref: '#/components/schemas/ISWCMetadata'\r\n        potentialMatches:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/ISWCMetadata'\r\n        multipleAgencyWorkCodes:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              agency:\r\n                type: string\r\n                minLength: 3\r\n                maxLength: 3\r\n              workCode:\r\n                type: string\r\n                maxLength: 20\r\n              rejection:\r\n                $ref: '#/components/schemas/Rejection'\r\n        alternateIswcMatches:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/AlternateIswcMatch'\r\n    SubmissionBatch:\r\n      type: object\r\n      properties:\r\n        submissionId:\r\n          type: integer\r\n          format: int32\r\n          description: A unique submission identifier for this request. The same identifier will be populated for the submission in the response.\r\n        submission:\r\n          $ref: '#/components/schemas/Submission'\r\n        multipleAgencyWorkCodes:\r\n          type: array\r\n          items:\r\n            type: object\r\n            properties:\r\n              agency:\r\n                type: string\r\n                minLength: 3\r\n                maxLength: 3\r\n              workCode:\r\n                type: string\r\n                maxLength: 20\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n      xml:\r\n        name: Submission\r\n    VerifiedSubmission:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonSubmissionAttributes\"\r\n      - $ref: \"#/components/schemas/CommonSystemAttributes\"\r\n      - type: object\r\n      required:\r\n        - id\r\n        - iswc\r\n        - deleted\r\n      properties:\r\n        id:\r\n          description: \"An unique identifier assigned by the system\"\r\n          type: integer\r\n          format: int64\r\n        iswc:\r\n          description: \"A preferred ISWC assigned by the system\"\r\n        iswcEligible:\r\n          description: \"Indicator of a submitter being Iswc Eligible\"\r\n          type: boolean\r\n        iswcStatus:\r\n          description: \"Indicator of ISWC being preferred, provisional or archived\"\r\n          type: string\r\n        deleted:\r\n          description: \"Indicator of a submission being logically deleted on CSINET\"\r\n          type: boolean\r\n        linkedFrom:\r\n          description: \"The ISWC record that this is linked from\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        linkedTo:\r\n          description: \"The ISWC record that this is linked to\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        rejection:\r\n          $ref: \"#/components/schemas/Rejection\"\r\n        archivedIswc:\r\n          description: \"The Archived ISWC\"\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        overallParentISWC:\r\n              description: \"The preferred ISWC that is at the top of the chain of ISWCs.\"\r\n              type: string\r\n              pattern: 'T[0-9]{10}'\r\n    VerifiedSubmissionBatch:\r\n      type: object\r\n      properties:\r\n        submissionId:\r\n          type: integer\r\n          format: int32\r\n          description: The value of the submissionID from the request submission.\r\n        submission:\r\n          $ref: '#/components/schemas/SubmissionResponse'\r\n        rejection:\r\n          $ref: '#/components/schemas/Rejection'\r\n      xml:\r\n        name: Submission\r\n    AlternateIswcMatch:\r\n      allOf:\r\n      - $ref: '#/components/schemas/CommonISWCMetadata'\r\n      - type: object\r\n    CommonSubmissionAttributes:\r\n      allOf:\r\n      - $ref: \"#/components/schemas/CommonISWCMetadata\"\r\n      - type: object\r\n      required:\r\n        - agency\r\n        - sourcedb\r\n        - category\r\n        - originalTitle\r\n        - interestedParties\r\n      properties:\r\n        agency:\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3\r\n        sourcedb:\r\n          type: integer\r\n          format: int32\r\n        workcode:\r\n          type: string\r\n        category:\r\n          type: string\r\n          enum:\r\n          - DOM\r\n          - INT\r\n        disambiguation:\r\n          type: boolean\r\n        disambiguationReason:\r\n          $ref: '#/components/schemas/DisambiguationReason'\r\n        disambiguateFrom:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/DisambiguateFrom'\r\n        bvltr:\r\n          $ref: '#/components/schemas/BVLTR'\r\n        derivedWorkType:\r\n          $ref: '#/components/schemas/DerivedWorkType'\r\n        derivedFromIswcs:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/DerivedFrom'\r\n        performers:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/Performer'\r\n        instrumentation:\r\n          type: array\r\n          items:\r\n            $ref: '#/components/schemas/Instrumentation'\r\n        cisnetCreatedDate:\r\n          description: \"Date and time when this metadata was created on CISNET\"\r\n          type: string\r\n          format: date-time\r\n        cisnetLastModifiedDate:\r\n          description: \"Date and time when this metadata last modified on CISNET\"\r\n          type: string\r\n          format: date-time\r\n        additionalIdentifiers:\r\n          description: \"Optional list of additional identifiers that can be supplied with the submission.\"\r\n          type: object\r\n          properties:\r\n            isrcs:\r\n              type: array\r\n              items:\r\n                type: string\r\n            publisherIdentifiers:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  nameNumber:\r\n                    type: integer\r\n                    format: int64\r\n                  submitterCode:\r\n                    type: string\r\n                  workCode:\r\n                    type: array\r\n                    items:\r\n                      type: string\r\n            agencyWorkCodes:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  agency:\r\n                    type: string\r\n                    minLength: 3\r\n                    maxLength: 3\r\n                  workCode:\r\n                    type: string\r\n                    maxLength: 20\r\n            labelIdentifiers:\r\n              type: array\r\n              items:\r\n                type: object\r\n                properties:\r\n                  submitterDPID:\r\n                    type: string\r\n                  workCode:\r\n                    type: array\r\n                    items:\r\n                      type: string     \r\n            recordings:\r\n              type: array\r\n              items:\r\n                type: object\r\n                required:\r\n                  - isrc\r\n                  - recordingTitle\r\n                properties:\r\n                  isrc:\r\n                    type: string\r\n                  recordingTitle:\r\n                    type: string\r\n                  subTitle:\r\n                    type: string\r\n                  labelName:\r\n                    type: string\r\n                  releaseEmbargoDate:\r\n                    type: string\r\n                    format: date-time\r\n                  performers:\r\n                    type: array\r\n                    items:\r\n                      $ref: '#/components/schemas/Performer'\r\n    CommonISWCMetadata:\r\n      required:\r\n        - originalTitle\r\n        - interestedParties\r\n      properties:\r\n        iswc:\r\n          type: string\r\n          pattern: 'T[0-9]{10}'\r\n        iswcStatus:\r\n          description: \"Indicator of ISWC being preferred, provisional or archived\"\r\n          type: string\r\n        agency:\r\n          description: A CISAC code of a submitting agency that allocated this ISWC\r\n          type: string\r\n          minLength: 3\r\n          maxLength: 3      \r\n        original